<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gallery.jungle.kalimah">

    <application
        android:allowBackup="true"
        android:icon="@drawable/open"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.gallery.jungle.kalimah.Open"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.gallery.jungle.kalimah.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.gallery.jungle.kalimah.KalimahTayyibah" >
        </activity>
        <activity android:name="com.gallery.jungle.kalimah.KalimahShahadah" >
        </activity>
        <activity android:name="com.gallery.jungle.kalimah.KalimahTamjeed" >
        </activity>
        <activity android:name="com.gallery.jungle.kalimah.KalimahTawheed" >
        </activity>
        <activity android:name="com.gallery.jungle.kalimah.KalimahIstighfar" >
        </activity>
        <activity android:name="com.gallery.jungle.kalimah.KalimaRaddeKufr" >
        </activity>
    </application>

</manifest>